          <div class="container">

            <div id="toolbar">
              <select class="form-control">
                <option value="">Export Basic</option>
                <option value="all">Export All</option>
                <option value="selected">Export Selected</option>
              </select>
            </div>

            <table id="table" data-toggle="table" data-search="true" data-filter-control="true" data-show-export="true"
              data-click-to-select="true" data-toolbar="#toolbar" class="table-responsive">
              <thead class="thead-light">
                <tr>
                  <th>Order ID</th>
                  <th>Customer</th>
                  <th>Amount</th>
                  <th>Status</th>


                </tr>
              </thead>
              <tbody>
                <%if(List){%>
                  <% List.forEach((item)=> { %>

                    <tr>
                      <td>
                        <%= item.orderNumber %>
                      </td>
                      <td>
                        <%= item.orderaddress.name%>
                      </td>
                      <td>
                        <%= item.totalprice %>
                      </td>
                      <td>
                        <% if( item.orderstatus=="Delivered" ){ %>
                          <button class="btn btn-success btn-equal-size">
                            <%= item.orderstatus %>
                          </button>
                          <% }else if( item.orderstatus=="shipping" ){ %>
                            <span class="badge badge-info btn-equal-size p-3">
                              <%=item.orderstatus%>
                            </span>
                            <% }else if( item.orderstatus=="Out for Delivery" ){ %>
                              <span class="badge badge-warning btn-equal-size p-3 ">
                                <%=item.orderstatus%>
                              </span>
                              <% }else if( item.orderstatus=="pending" ){ %>
                                <span class="badge badge-danger btn-equal-size p-3">
                                  <%=item.orderstatus%>
                                </span>
                                <% }else if( item.orderstatus=="cancel" ){ %>
                                  <span class="badge badge-warning btn-equal-size p-3">
                                    <%=item.orderstatus%>
                                  </span>
                                  <% }else if( item.orderstatus=="return" ){ %>
                                    <span class="badge badge-danger btn-equal-size p-3">
                                      <%=item.orderstatus%>
                                    </span>

                                    <% } %>
                      </td>
                    </tr>

                    <% }) %>
                      <% } %>


              </tbody>
            </table>
